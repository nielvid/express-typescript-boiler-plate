{
  "name": "express-typescript-boiler-plate",
  "version": "1.0.0",
  "description": "Express App with Typescript boiler-plate",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc --project src/",
    "start": "concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "start:dev": "npm start",
    "start:prod": "node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:nielvid/express-typescript-boiler-plate.git"
  },
  "author": "Nielvid",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "rimraf": "^3.0.2"
  }
}
